{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.30.2",
  "yarnVersion": "1.9.4",
  "nodeSupportedVersionRange": ">=10.13.0 <13.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "repository": {
     "url": "https://github.com/dbartholomae/lambda-middleware.git",
     "defaultBranch": "master",
     "defaultRemote": "origin"
  },
  "projects": [{
    "packageName": "@lambda-middleware/http-error-handler",
    "projectFolder": "packages/http-error-handler",
    "shouldPublish": true
  }, {
    "packageName": "@lambda-middleware/json-serializer",
    "projectFolder": "packages/json-serializer",
    "shouldPublish": true
  }, {
    "packageName": "@lambda-middleware/compose",
    "projectFolder": "packages/compose",
    "shouldPublish": true
  }, {
    "packageName": "@lambda-middleware/jwt-auth",
    "projectFolder": "packages/jwt-auth",
    "shouldPublish": true
  }, {
    "packageName": "@lambda-middleware/class-transformer",
    "projectFolder": "packages/class-transformer",
    "shouldPublish": true
  }]
}
